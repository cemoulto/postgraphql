"schema {
  query: Query
}

type CompoundKey implements Node {
  __id: ID!
  personId2: Int!
  personId1: Int!
}

scalar Email

interface Node {
  __id: ID!
}

type NoUpdate {
  column: Int
}

type Person implements Node {
  __id: ID!
  id: Int!
  name: String!
  about: String
  email: Email
  createdAt: String
}

type Post implements Node {
  __id: ID!
  id: Int!
  headline: String!
  body: String
